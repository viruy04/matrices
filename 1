#include <stdio.h>
int main() {
    int matrix1[3][3];
    int matrix2[3][3];
    int matrix3[3][3];
    int matrix4[3][3];
    printf("Введите элементы первой матрицы 3x3:\n");
    for(int i=0; i<3; i++) {
        for(int j=0; j<3; j++) {
            scanf("%d", &matrix1[i][j]);
        }
    } 
    printf("Введенная матрица 1:\n");
    for(int i=0; i<3; i++) {
        for(int j=0; j<3; j++) {
            printf("%d ", matrix1[i][j]);
        }
        printf("\n");
    }

    printf("Введите элементы второй матрицы 3x3:\n");
    for(int i=0; i<3; i++) {
        for(int j=0; j<3; j++) {
            scanf("%d", &matrix3[i][j]);
        }
    } 
    printf("Введенная матрица 2:\n");
    for(int i=0; i<3; i++) {
        for(int j=0; j<3; j++) {
            printf("%d ", matrix3[i][j]);
        }
        printf("\n");
    }
    printf("Транспонированная матрица 2:\n");
    for(int i=0; i<3; i++) {
        for(int j=0; j<3; j++) {
            matrix4[i][j] = matrix3[j][i];
            printf("%d ", matrix4[i][j]);
        }
        printf("\n");
    }
    printf("Транспонированная матрица 1:\n");
    for(int i=0; i<3; i++) {
        for(int j=0; j<3; j++) {
            matrix2[i][j] = matrix1[j][i];
            printf("%d ", matrix2[i][j]);
        }
        printf("\n");
    } 
    return 0;
}
